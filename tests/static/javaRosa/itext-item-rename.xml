<?xml version="1.0" encoding="UTF-8" ?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Untitled Form</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/09ACF7A7-5D23-41F7-BFF6-E6977A5CA913" uiVersion="1" version="1" name="Untitled Form">
					<ns />
					<ew />
				</data>
			</instance>
			<bind nodeset="/data/ns" />
			<bind nodeset="/data/ew" />
			<itext>
				<translation lang="en" default="">
					<text id="ns-label">
						<value>ns</value>
					</text>
					<text id="ns-north-label">
						<value>item1</value>
					</text>
					<text id="ns-south-label">
						<value>item1</value>
					</text>
					<text id="ns-item2-label">
						<value>item2</value>
					</text>
					<text id="ew-label">
						<value>ew</value>
					</text>
					<text id="ew-item2-label">
						<value>item2</value>
					</text>
				</translation>
				<translation lang="hin">
					<text id="ns-label">
						<value>ns</value>
					</text>
					<text id="ns-north-label">
						<value>item1</value>
					</text>
					<text id="ns-south-label">
						<value>item1</value>
					</text>
					<text id="ns-item2-label">
						<value>item2</value>
					</text>
					<text id="ew-label">
						<value>ew</value>
					</text>
					<text id="ew-item2-label">
						<value>item2</value>
					</text>
				</translation>
			</itext>
		</model>
	</h:head>
	<h:body>
		<select1 ref="/data/ns">
			<label ref="jr:itext('ns-label')" />
			<item>
				<label ref="jr:itext('ns-north-label')" />
				<value>north</value>
			</item>
			<item>
				<label ref="jr:itext('ns-south-label')" />
				<value>south</value>
			</item>
			<item>
				<label ref="jr:itext('ns-item2-label')" />
				<value>item2</value>
			</item>
		</select1>
		<select1 ref="/data/ew">
			<label ref="jr:itext('ew-label')" />
			<item>
				<label ref="jr:itext('ew-item2-label')" />
				<value>item2</value>
			</item>
		</select1>
	</h:body>
</h:html>